{
  "name": "upload-release-asset",
  "version": "1.0.0",
  "private": true,
  "description": "Upload a release asset to an existing GitHub Release",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brian-pickens/upload-release-asset"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "Brian Pickens",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "@octokit/plugin-retry": "^3.0.9",
    "@octokit/plugin-throttling": "^3.5.1",
    "glob": "^7.1.6",
    "mime": "^2.4.4",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.25",
    "@types/mime": "^2.0.1",
    "@types/node": "^16.10.5",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/parser": "^5.8.1",
    "@vercel/ncc": "^0.31.1",
    "@zeit/ncc": "^0.21.0",
    "eslint": "^8.0.1",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "jest": "^28.1.1",
    "jest-circus": "^24.9.0",
    "prettier": "1.19.1",
    "ts-jest": "^28.0.5",
    "typescript": "^4.2.0",
    "typescript-formatter": "^7.2.2"
  }
}
